var timeLimitWrap = d.getElementById('js_timeLimitWrap');
addClass(timeLimitWrap, 'opacity0');
return;
}
else if(timeLimit > 0) {
var time = this.addZero(hours) + ':' + this.addZero(minutes) + ':' + this.addZero(seconds);
limitEl.innerHTML = time;
var stop = setTimeout(function(){
that.countDown(timeLimit);
},1000);
} else if(timeLimit === 0) {
location.reload(true);
} else {
return;
}
timeLimit -= 1;
},
/**
* ゼロを付与
*/
addZero: function(num){
num = '00' + num;
str = num.substring(num.length - 2, num.length);
return str;
}
};
countDownStamina.countDown(ftl_staminaTimeLimit);
})();
