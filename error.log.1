countDown: function(timeLimit){
var limitEl = d.getElementById('js_timeLimitStamina'),
hours = Math.floor(timeLimit / (60 * 60)),
minutes = Math.floor(timeLimit % (60 * 60) / 60),
seconds = Math.floor(timeLimit % (60 * 60) % 60),
that = this;
